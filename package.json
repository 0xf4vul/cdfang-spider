{
  "name": "cdfang-spider",
  "version": "1.3.2",
  "description": "成都房协网爬虫，定时爬取最新房源，可视化数据分析。",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "produce-source-map": true
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "client": "webpack-dev-server --open",
    "server": "cross-env NODE_ENV=development gulp & supervisor ./dist/app.js",
    "start": "cross-env NODE_ENV=development gulp & webpack-dev-server --open & supervisor ./dist/app.js & wait",
    "prebuild": "npm run lint && rimraf ./dist",
    "build": "cross-env NODE_ENV=production gulp & cross-env NODE_ENV=production webpack",
    "test": "nyc mocha --recursive",
    "coverage": "codecov"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhlben/cdfang-spider.git"
  },
  "author": "yhlben",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yhlben/cdfang-spider/issues"
  },
  "homepage": "https://github.com/yhlben/cdfang-spider#readme",
  "dependencies": {
    "@antv/data-set": "^0.10.1",
    "antd": "^3.10.7",
    "bizcharts": "^3.2.4",
    "cheerio": "^1.0.0-rc.2",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.5",
    "lodash": "^4.17.11",
    "mobx": "^5.8.0",
    "mobx-react": "^5.3.6",
    "moment": "^2.21.0",
    "mongoose": "^5.3.12",
    "node-schedule": "^1.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "superagent": "^4.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^1.3.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "qiniu": "^7.2.1",
    "qiniu-upload-plugin": "^1.0.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "supervisor": "^0.12.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
